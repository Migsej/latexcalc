number = @{
    "-"?
    ~ ("0" | ASCII_NONZERO_DIGIT ~ ASCII_DIGIT*)
    ~ ("." ~ ASCII_DIGIT*)?
    ~ (^"e" ~ ("+" | "-")? ~ ASCII_DIGIT+)?
}

fraction = { "\\frac{" ~ equation ~ "}{" ~ equation ~ "}" }

multiply = { "\\cdot" | "*" }
plus = { "+" }
minus = { "-" }

operation = { fraction | multiply }
equation = {(fraction | multiply | number)+ } 

WHITESPACE = _{ " " }

